{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "./",
    "target": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@@/*": [".umi/*"]
    }
  },
  "exclude": ["node_modules", "lib", "es"]
}
